                        :append-icon=\"showPass ? 'mdi-eye' : 'mdi-eye-off'\"\n                        @click:append=\"showPass = ! showPass\"/>\n                    </v-form>\n                </v-card-text>\n                <v-divider></v-divider>\n                <v-card-actions>\n                    <v-spacer></v-spacer>\n                    <v-btn color=\"info\" @click=\"signInVue\">Login</v-btn>\n                </v-card-actions>\n            </v-card>\n        </v-row>\n    </v-container>\n</template>\n\n<script>\nimport {mapGetters, mapState} from 'vuex';\n\nexport default {\n    name: \"Login\",\n    computed: {\n        ...mapGetters(['getAuthtoken', 'getIsConnected'])\n    },\n    data(){\n        return {\n            ...mapState(['authtoken']),\n            showPass: false,\n            loginInfo: {\n                userName: \"\",\n                passWord: \"\"\n            }\n        }\n    },\n    methods: {\n        async signInVue(){\n            let user = await this.$store.dispatch(\"signIn\", this.loginInfo);\n            alert(user.authToken);\n            alert(this.$store.state.auth.authtoken);\n        }\n    }\n}\n</script>\n\n<style>\n\n</style>"]}]}